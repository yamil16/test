[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/yamil16
[access "refs/for/refs/*"]
	read = group user/yamil16
	push = group user/yamil16
	label-Code-Review = -2..+2 group user/yamil16
	label-Verified = -1..+1 group user/yamil16
	rebase = group user/yamil16
	removeReviewer = group user/yamil16
	submit = group user/yamil16
[access "refs/heads/*"]
	read = group user/yamil16
	create = group user/yamil16
	push = +force group user/yamil16
	pushMerge = group user/yamil16
[access "refs/tags/*"]
	push = group user/yamil16
	push = +force group user/yamil16
